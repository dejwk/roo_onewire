{
    "name": "roo_onewire",
    "description": "Arduino library for OneWire thermometers. Supports asynchronous conversion with event notification, and persistent configuration to dynamically (re)assign static thermometer 'roles' to specific devices.",
    "keywords": "OneWire, temperature, DS18B20",
    "authors": [
        {
            "name": "Dejwk",
            "email": "dejwk.oss@gmail.com",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dejwk/roo_onewire"
    },
    "version": "1.0.1",
    "homepage": "https://github.com/dejwk/roo_onewire",
    "frameworks": [
        "Arduino",
        "*"
    ],
    "build": {
        "srcDir": "src",
        "srcFilter": "+<*> -<examples> -<test>"
    },
    "examples": [
        "examples/*/*.ino"
    ],
    "dependencies": {
        "OneWire": ">0",
        "dejwk/roo_collections": ">=1.1.5",
        "dejwk/roo_logging": ">=1.0.5",
        "dejwk/roo_prefs": ">=1.0.2",
        "dejwk/roo_scheduler": ">=1.2.1",
        "dejwk/roo_temperature": ">=1.1.0"
    }
}
